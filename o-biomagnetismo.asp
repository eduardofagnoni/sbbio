            <section class="nome-da-pagina">
                <h1>O Biomagnetismo</h1>
            </section>

            <div class="curve-top">
                <img src="images/curva.png" alt="A curvatura do bloco" class="curva">
                <img src="images/curva-menor.png" alt="A curvatura do bloco" class="curva-menor">
